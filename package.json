{
  "name": "velloware-dash",
  "version": "1.0.0",
  "description": "Dashboard for velloware",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "dev:status": "ts-node-dev --transpile-only --ignore-watch node_modules src/modules/Status/Status.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll --maxWorkers=1",
    "test:clear": "jest --clearCache",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:createOnly": "npx prisma migrate dev --create-only",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@prisma/client": "^4.7.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node-cron": "^3.0.4",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express-async-errors": "^3.1.1",
    "node-cron": "^3.0.2",
    "prisma": "^4.7.1",
    "telegraf": "^4.8.6"
  }
}
