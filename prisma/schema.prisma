// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" //env("DATABASE_URL") //file:./dev.db
}

model Alert {
  id       String @id @unique @default(uuid())
  title    String
  priority String
  impactedEntities String
  totalIncidents Int
  state String
  trigger String
  isCorrelated Boolean
  issueUrl String
  createdAt String
  updatedAt String
  sources String
  alertPolicyNames String
  alertConditionNames String
  workflowName String
}